#![allow(clippy::needless_raw_string_hashes, clippy::no_effect, unused)]
#![warn(clippy::needless_raw_strings)]

fn main() {
    "aaa"; //~ needless_raw_strings
    r#""aaa""#;
    r#"\s"#;
    b"aaa"; //~ needless_raw_strings
    br#""aaa""#;
    br#"\s"#;
    c"aaa"; //~ needless_raw_strings
    cr#""aaa""#;
    cr#"\s"#;

    //~v needless_raw_strings
    "
        a
        multiline
        string
    ";

    "no hashes"; //~ needless_raw_strings
    b"no hashes"; //~ needless_raw_strings
    c"no hashes"; //~ needless_raw_strings
}
